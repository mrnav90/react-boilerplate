{
  "name": "react-boilerplate",
  "version": "2.0",
  "author": "VietNa",
  "description": "React boilerplate",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --https --hot --progress",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "test:watch": "jest --watchAll --verbose",
    "test:coverage": "jest --verbose --coverage",
    "build:dev": "webpack --watch --hot --progress",
    "start": "cross-env NODE_ENV=production webpack -p --progress --optimize-max-chunks 15 --optimize-min-chunk-size 10000 --env.prod=true",
    "eslint": "./node_modules/.bin/eslint --ext .jsx,.js src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrnav90/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=10.16.3"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint"
    }
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
